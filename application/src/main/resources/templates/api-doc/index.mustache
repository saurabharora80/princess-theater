<!DOCTYPE html>
<!-- Bedrock file. DO NOT DELETE OR MODIFY. -->
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Swagger UI</title>
    <link rel="stylesheet" type="text/css" href="{{contextPath}}/webjars/swagger-ui/swagger-ui.css">
    <style>
        html {
            box-sizing: border-box;
            overflow: -moz-scrollbars-vertical;
            overflow-y: scroll;
        }

        *,
        *:before,
        *:after {
            box-sizing: inherit;
        }

        body {
            margin: 0;
            background: #fafafa;
        }
    </style>
</head>

<body>
<div id="swagger-ui"></div>
<script src="{{contextPath}}/webjars/swagger-ui/swagger-ui-bundle.js"></script>
<script src="{{contextPath}}/webjars/swagger-ui/swagger-ui-standalone-preset.js"></script>
<script>
    window.onload = function () {
        window.ui = SwaggerUIBundle({
            url: "openapi.yaml",
            dom_id: '#swagger-ui',
            deepLinking: true,
            presets: [
                SwaggerUIBundle.presets.apis,
                SwaggerUIStandalonePreset
            ],
            plugins: [
                SwaggerUIBundle.plugins.DownloadUrl
            ],
            layout: "BaseLayout",
            sorter: "alpha",
            docExpansion: "list",
            defaultModelsExpandDepth: 0
        });
    }
</script>
</body>
</html>
